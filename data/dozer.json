{
    "name": "dozer",
    "id": "com.boostedhuman.robots.dozer",
    "components": {

      "dozer_node": {
        "classname": "dozer_node.DozerNode",
        "deps": ["rpc_proxy", "hardware_service"]
      },

      "rpc_proxy": {
        "classname": "rpc_proxy_node.RPCProxyNode",
        "params": {
          "port": 8080
        }
      },

      "hardware_service": {
        "classname": "hardware_service.HardwareService",
        "params": {
          "stub": false,
          "serial_port": "/dev/tty.usbmodem1421",
          "serial_port_speed": 9600,
          "service_port": 9000
        }
      }
    }
}
